_start:
    lit 0
    lit 100
    !
    lit 1
    lit 110
    !
    lit 2
    lit 0
    lit 4
    lit 5
    !
while:
    lit 0 @
    lit 1 @
    *
    lit 3
    swap
    !
    jump palindrome_save
palindrome_save:
    lit 3 @
    lit 10
    swap
    mod
    lit 4 @
    swap
    !
    lit 10
    /
    dup
    if palindrome_check
    lit 3
    swap
    !
    lit 4
    dup
    lit 1
    +
    !
    jump palindrome_save
palindrome_check:
    halt