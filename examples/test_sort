_start:
    key 0
    lit 48  ; чтобы сделать нормальное количество букв (паскаль строкой)
    call rep
    dup
    lit 3
    swap
    !
    !
    lit 4
    lit 0
    !
while:
    key 0
    lit 2
    @
    lit 3
    @       ; это нужно чтобы найти число, которое прибавить к адресу
    -
    lit 7   ; адрес массива
    +
    swap
    !
    lit 2
    @
    lit 1
    call rep
    dup
    if next
    !
    lit 4
    dup
    @
    lit 3
    +
    jump while
rep:
    swap
    -
    lit 2
    swap
    ret
next:
    lit 3
    @
    lit 2
    swap
    !
repeat:
    lit 5
    @
    lit 1
    +
    lit 2
    @
    -
    if check
    call get_nums
    swap
    >
    dup
    if swap_nums
    lit 6
    @
    +
    lit 6
    swap
    !
    jump continue
continue:
    lit 5
    dup
    @
    lit 1
    +
    !
    jump repeat
swap_nums:
    call get_nums
    swap
    call get_addr
    swap
    !
    call get_addr
    lit 1
    +
    swap
    !
    jump continue
get_addr:
    lit 7   ; адрес массива
    lit 5
    @
    +
    ret
get_nums:
    call get_addr
    dup
    lit 1
    +
    @
    swap
    @
    ret
check:
    lit 6
    @
    lit 1
    +
    lit 2
    @
    -
    if end
    lit 6
    lit 0
    !
    lit 5
    lit 0
    !
    jump repeat
end:
    halt